{
    "name": "react-router-loading",
    "version": "0.5.0",
    "description": "Custom react router switch that allows you to load data before switching the screen",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepublish": "cross-env NODE_ENV=production yarn build",
        "prebuild": "rimraf dist/",
        "build": "tsc --skipLibCheck && yarn babel:code && yarn babel:examples",
        "webpack": "webpack ./example/dist/index.js -o ./example/dist --mode=development --devtool source-map",
        "start": "yarn build && http-server-spa ./example",
        "babel:code": "babel ./src --presets=@babel/preset-typescript --extensions \".ts,.tsx\" --out-dir ./dist -s inline",
        "babel:examples": "babel ./example/src --out-dir ./example/dist -s inline && npm run webpack"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jayarjo/react-router-loading.git"
    },
    "files": [
        "dist/*",
        "index.d.ts"
    ],
    "keywords": [
        "react",
        "switch",
        "router",
        "loading",
        "fetch",
        "suspense",
        "react-router-dom",
        "preload",
        "prefetch",
        "prerender",
        "topbar",
        "bar"
    ],
    "author": "Victor Trusov",
    "contributors": [
        {
            "name": "Davit Barbakadze"
        }
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jayarjo/react-router-loading/issues"
    },
    "homepage": "https://github.com/jayarjo/react-router-loading#readme",
    "peerDependencies": {
        "react": "^16.8.0 || ^17.0.0",
        "react-dom": "^16.8.0 || ^17.0.0",
        "react-router-dom": "^6.3.0"
    },
    "dependencies": {
        "topbar": "^1.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.5",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.16.7",
        "babel-eslint": "^10.1.0",
        "cross-env": "7.0.3",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-config-standard-react": "^11.0.1",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-react": "^7.26.1",
        "http-server-spa": "1.3.0",
        "node": "16.10.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-router-dom": "^6.3.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.6.4",
        "webpack": "5.55.1",
        "webpack-cli": "4.8.0"
    }
}
